{
  "schemaVersion": 1,
  "id": "muxel-enhancements",
  "version": "${version}",
  "name": "Muxel-Enhancements",
  "description": "Enhancements Mod for improved functionality especially for the mixelpixel minecraft server. This mod has no association to the Mixelpixel Gbr itself and is not officially supported by Mixelpixel",
  "authors": ["Klysma", "Yve"],
  "contact": {},
  "license": "MIT",
  "icon": "assets/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "click.isreal.topbar.client.TopbarClient"
    ],
    "main": [
      "click.isreal.topbar.Topbar"
    ],
    "modmenu": [
      "click.isreal.topbar.ModMenuApiImplementation"
    ]
  },
  "mixins": [
    "topbar.mixins.json"
  ],
  "depends": {
    "cloth-config2": ">=9.0.94",
    "fabricloader": ">=0.14.11",
    "fabric": "*",
    "minecraft": ">=1.19.3"
  }
}
